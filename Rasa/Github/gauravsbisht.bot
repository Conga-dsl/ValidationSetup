entities :
	Regex entity email_id :
		inputs in en {
			pattern : "[A-Z0-9a-z._-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,64}"
	}
	Regex entity pincode :
		inputs in en {
			pattern : "[0-9]{6}"
	}
	Regex entity location :
		inputs in en {
			pattern : "[a-zA-Z ][-][ a-zA-Z]*"
	}

actions :
	Text response utter_wrong_city_text :
		inputs in en {
			"City is not in our database"
	}
	Text response utter_greet_text :
