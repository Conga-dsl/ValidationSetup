	- user greet => chatbot utter_greet_text {
		=> user request_restaurant => chatbot action_search_restaurants_empty , utter_ask_email_id_text , utter_ask_email_text , DefaultEmptyAction { => user email_affirm_intent => chatbot action_send_email_empty { => user thankyou => chatbot DefaultEmptyAction ; } ; => user formlocation_intent => chatbot DefaultEmptyAction , action_search_restaurants_empty , utter_ask_email_id_text , utter_ask_email_text { => user formcuisine_intent => chatbot DefaultEmptyAction , action_search_restaurants_empty , utter_ask_email_id_text , utter_ask_email_text { => user formbudget_intent => chatbot action_search_restaurants_empty , utter_ask_email_id_text , utter_ask_email_text { => user email_affirm_intent => chatbot action_send_email_empty { => user thankyou => chatbot DefaultEmptyAction , utter_goodbye_text ; } ; } ; => user email_affirm_intent => chatbot utter_ask_email_id_text { => user email_id_intent => chatbot action_send_email_empty ; } ; } ; => user email_affirm_intent => chatbot utter_goodbye_text , utter_restart_text ; } ; } ;
	} ;
	- user request_restaurant => chatbot utter_ask_location_text {
		=> user location_intent => chatbot DefaultEmptyAction { => user formcuisine_intent => chatbot DefaultEmptyAction { => user formbudget_intent => chatbot action_search_restaurants_empty , utter_ask_email_id_text , utter_ask_email_text { => user email_affirm_intent => chatbot action_send_email_empty { => user thankyou => chatbot DefaultEmptyAction ; } ; } ; } ; } ;
	} ;

