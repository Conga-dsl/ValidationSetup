entities :
	Regex entity pincode :
		inputs in en {
			pattern : "[0-9]{6}"
	}
	Regex entity vegetarian :
		inputs in en {
			pattern : veg
			pattern : vegan
			pattern : vegg
			pattern : veggie
	}
	Regex entity greet :
		inputs in en {
			pattern : "hey[^\\s]*"
	}
	Regex entity email :
		inputs in en {
			pattern : "^[a-z0-9]+[._]?[a-z0-9]+[@]\\w+[.]\\w{2,3}$"
	}

actions :
	Empty response action_check_location_empty
	Text response utter_email_prompt_text :
		inputs in en {
			"Should I send you details of all the restaurants on email?"
	}
	Text response utter_greet_text :
		inputs in en {
			"Heyy, what is that your looking today?" ,
			"How can I help you today?" ,
			"Hi There!!! How can I help you?" ,
			"How is your day going" ,
			"How are you doing?" ,
			"Good Day"
	}
	Text response utter_ask_price_text :
		inputs in en {
			"What is the average budget for two people?"
	}
	Empty response action_send_email_empty
	Text response utter_ask_cuisine_text :
		inputs in en {
			"What kind of cuisine do you prefer?"
	}
	Text response utter_ask_email_text :
		inputs in en {
			"please key in your email" ,
			"Email please" ,
			"Type your email"
	}
	Empty response action_search_restaurants_empty
	Text response utter_ask_location_text :
		inputs in en {
			"In what location?" ,
			"In which city are you looking for restaurants?" ,
			"location please" ,
			"Help me with your location to proceed further"
	}
	Text response utter_default_text :
		inputs in en {
			"We do not operate in that area yet" ,
			"I cannot uderstand"
	}
	Text response utter_goodbye_text :
		inputs in en {
			"Bye bye" ,
			"Have a nice rest of the day" ,
			"Enjoy your food" ,
			"Good Bye"
	}

