flows :
	- user greet_hello => chatbot utter_greet_text {
		=> user mood_great => chatbot utter_happy_text {
			=> user reflect_ask_remember => chatbot utter_reflect_ask_remember_text ;
		} ;
		=> user mood_unhappy => chatbot utter_sad_text , utter_cheer_up_img , utter_did_that_help_text {
			=> user affirm => chatbot utter_happy_text ;
			=> user deny => chatbot utter_goodbye_text ;
		} ;
		=> user ask_howold => chatbot utter_ask_howold_text {
			=> user feeling_love => chatbot utter_handlelove_text ;
		} ;
		=> user my_name_is => chatbot utter_nicetomeetyou_text , utter_nicetomeetyoutoo_text {
			=> user ask_whatsmyname => chatbot utter_ask_whatsmyname_text ;
		} ;
		=> user ask_whoisit => chatbot utter_ask_whoisit_text , utter_idontknowname_text {
			=> user my_name_is => chatbot utter_nicetomeetyou_text , utter_nicetomeetyoutoo_text {
				=> user ask_whatsmyname => chatbot utter_ask_whatsmyname_text ;
			} ;
			=> user nicetomeetyou => chatbot utter_idontknowname_text {
				=> user my_name_is => chatbot utter_nicetomeetyou_text , utter_nicetomeetyoutoo_text { => user ask_whatsmyname => chatbot utter_ask_whatsmyname_text ; } ;
			} ;
		} ;
		=> user ask_whatsmyname => chatbot utter_ask_whatsmyname_text {
			=> user deny => chatbot utter_idontknowname_text { => user my_name_is => chatbot utter_nicetomeetyou_text , utter_nicetomeetyoutoo_text { => user affirm => chatbot utter_nicetomeetyoutoo_text ; } ; } ;
		} ;
		=> user ask_weather => chatbot utter_ask_location_text , action_get_weather_empty {
			=> user ask_weather => chatbot action_get_weather_empty {
				=> user greet_goodbye => chatbot utter_goodbye_text ;
			} ;
			=> user greet_goodbye => chatbot utter_goodbye_text ;
		} ;
		=> user ask_news => chatbot action_get_news_empty {
			=> user thank => chatbot utter_thanks_response_text ;
			=> user greet_goodbye => chatbot utter_goodbye_text ;
		} ;
		=> user reflect_i_feel => chatbot utter_reflect_feel_text ;
	} ;
	- user greet_goodbye => chatbot utter_goodbye_text ;
	- user ask_realORask_whoisitORask_builderORask_howdoingORask_languagesORask_howoldORask_genderORask_whereliveORask_howbuiltORask_hobbiesORask_otherbotsORask_planORask_singORask_goodORask_restaurantORask_wherefromORask_openpodORask_secret => chatbot action_chitchat_empty , utter_nicetomeetyou_text , utter_nicetomeetyoutoo_text ;
	- user ask_joke => chatbot utter_ask_joke_text {
		=> user ask_one_more => chatbot utter_ask_joke_text ;
	} ;
	- user ask_time => chatbot action_get_time_empty ;
	- user ask_date => chatbot action_get_date_empty ;
	- user ask_days_in_month => chatbot action_get_daysinmonth_empty ;
	- user ask_howhelp => chatbot utter_howhelp_text ;
	- user out_of_scope => chatbot utter_out_of_scope_text ;
	- user opinionpositive => chatbot utter_positive_feedback_reaction_text ;
	- user sorry => chatbot utter_nosorry_text ;
	- user feeling_love => chatbot utter_handlelove_text ;
	- user feeling_insult => chatbot utter_handleinsult_text ;
	- user initiate_chitchat => chatbot utter_dialogstarter_text {
		=> user maybe => chatbot utter_maybe_text ;
		=> user thank => chatbot utter_thanks_response_text , utter_thank_empty ;
	} ;
	- user nicetomeetyou => chatbot utter_nicetomeetyoutoo_text ;
	- user my_name_is => chatbot utter_nicetomeetyou_text , utter_nicetomeetyoutoo_text ;
	- user ask_whatsmyname => chatbot utter_ask_whatsmyname_text {
		=> user deny => chatbot utter_idontknowname_text { => user my_name_is => chatbot utter_nicetomeetyou_text , utter_nicetomeetyoutoo_text ; } ;
	} ;
	- user ask_news => chatbot action_get_news_empty ;
	- user reflect_dream => chatbot utter_reflect_dream_text {
		=> user reflect_i_want => chatbot utter_reflect_want_text { => user reflect_i_remember => chatbot utter_reflect_remember_text { => user reflect_if_i => chatbot utter_reflect_if_i_text ; } ; } ;
	} ;
	- user reflect_i_cant => chatbot utter_reflect_cant_text {
		=> user reflect_everyone => chatbot utter_reflect_everyone_text { => user reflect_always => chatbot utter_reflect_always_text { => user reflect_am_i => chatbot utter_reflect_am_i_text ; } ; } ;
	} ;
	- user reflect_is_like => chatbot utter_reflect_is_like_text {
		=> user reflect_always => chatbot utter_reflect_always_text { => user reflect_i_feel => chatbot utter_reflect_feel_text ; } ;
	} ;
	- user reflect_ask_remember => chatbot utter_reflect_ask_remember_text {
		=> user reflect_never => chatbot utter_reflect_never_text ;
	} ;
	- user ask_motor_action => chatbot action_motortask_empty ;
	- user summon_assistant => chatbot action_tell_assistant_empty ;

